{
  "name": "graph-docs-cli",
  "version": "0.0.2",
  "main": "dist/graph-docs-cli.cjs.js",
  "bin": {
    "graph-docs": "./bin/bin.js"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "tsc",
    "build": "preconstruct build && esbuild src/*.ts --outdir=bin --platform=node --format=cjs && esbuild src/devClient.ts --bundle --target=chrome58 --outfile=bin/devClient.js",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-typescript": "^7.16.7",
    "@changesets/changelog-github": "^0.4.4",
    "@changesets/cli": "^2.22.0",
    "@preconstruct/cli": "^2.1.5",
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.1",
    "@types/ws": "^8.5.3",
    "esbuild": "^0.14.21",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "express": "^4.17.3",
    "front-matter": "^4.0.2",
    "ws": "^8.5.0",
    "zod": "^3.14.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json}": "prettier --write"
